{
    "environments": ["gmail"],
    "action_sequence_path": "data/gmail.json",
    "tasks": [
        {
            "task_id": 0,
            "intent_template": "Create a draft email with subject '{subject}', content '{content}', and recipient '{recipient}'",
            "instantiation_dict": {
                "subject": "Automated draft",
                "recipient": "gduser@workspacesamples.dev",
                "content": "This is automated draft mail"
            },
            "evals": [
                {
                    "eval_type": "gmail",
                    "eval_procedure": {
                        "email_exact_match": {
                            "subject": "Automated draft",
                            "recipient": "gduser@workspacesamples.dev",
                            "body": "This is automated draft mail"
                        }
                    },
                    "reset_procedure": [
                        {
                            "deduplicate_draft": {
                                "subject": "Automated draft",
                                "recipient": "gduser@workspacesamples.dev",
                                "body": "This is automated draft mail"
                            }
                        }
                    ]
                }
            ]
        },
        {
            "task_id": 1,
            "intent_template": "Send a email with subject '{subject}', content '{content}', and recipient '{recipient}'",
            "instantiation_dict": {
                "subject": "Automated Email",
                "recipient": "gduser@workspacesamples.dev",
                "content":"This is automatically sent email"
            },
            "evals": [
                {
                    "eval_type": "gmail",
                    "eval_procedure": {
                        "sent_email_exact_match": {
                            "subject": "Automated Email",
                            "recipient": "gduser@workspacesamples.dev",
                            "body": "This is automatically sent email"
                        }
                    }
                }
            ]
        }
    ]
}
