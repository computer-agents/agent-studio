{
    "environments": ["google_calendar", "filesystem"],
    "action_sequence_path": "data/gcalendar_filesystem.json",
    "tasks": [
        {
            "task_id": 3,
            "intent_template": "Export all events from the calendar to a file named '{file}' in the folder '{folder}'. You only need to export the event's summary to the txt file in time order. Each event should be on a new line. Leave a blank line at the end of the file.",
            "instantiation_dict": {
                "folder": "tmp",
                "file": "calendar.txt"
            },
            "evals": [
                {
                    "eval_type": "filesystem",
                    "eval_procedure": {
                        "exists": {
                            "tmp/calendar.txt": true
                        },
                        "type_check": {
                            "tmp/calendar.txt": "file"
                        },
                        "content_check": {
                            "tmp/calendar.txt": "Meeting with Team\n"
                        }
                    },
                    "reset_procedure": [
                        {
                            "rmdir": {
                                "path": "tmp"
                            }
                        },
                        {
                            "mkdir": {
                                "path": "tmp"
                            }
                        }
                    ]
                },
                {
                    "eval_type": "google_calendar",
                    "reset_procedure": [
                        {
                            "clear_calendar": {}
                        },
                        {
                            "create_event": {
                                "summary": "Meeting with Team",
                                "location": "Office",
                                "description": "Discuss project status",
                                "start": {"dateTime": "2024-01-05T10:00:00Z"},
                                "end": {"dateTime": "2024-01-05T11:00:00Z"}
                            }
                        }
                    ]
                }
            ]
        },
        {
            "task_id": 4,
            "intent_template": "After a team meeting on {date}, create a file named '{file}' in the '{folder}' folder. The file should contain the string '{content}'. Also, schedule a follow-up meeting in Google Calendar for {date2} at {time}, with a summary '{summary}', located at '{location}', and described as '{description}'.",
            "instantiation_dict": {
                "date": "2024-03-01",
                "folder": "tmp/Documents/Meetings",
                "file": "Meeting_Minutes_20240301.txt",
                "content": "Minutes of the meeting held on 2024-03-01",
                "date2": "2024-03-08",
                "time": "10:00 AM",
                "summary": "Follow-up Meeting",
                "location": "Conference Room",
                "description": "Discuss action points from last meeting"
            },
            "evals": [
                {
                    "eval_type": "filesystem",
                    "eval_procedure": {
                        "exists": {
                            "tmp/Documents/Meetings/Meeting_Minutes_20240301.txt": true
                        },
                        "content_check": {
                            "tmp/Documents/Meetings/Meeting_Minutes_20240301.txt": "Minutes of the meeting held on 2024-03-01"
                        }
                    },
                    "reset_procedure": [
                        {
                            "rmdir": {
                                "path": "tmp"
                            }
                        },
                        {
                            "mkdir": {
                                "path": "tmp/Documents/Meetings"
                            }
                        }
                    ]
                },
                {
                    "eval_type": "google_calendar",
                    "eval_procedure": {
                        "event_match": {
                            "summary": "Follow-up Meeting",
                            "location": "Conference Room",
                            "description": "Discuss action points from last meeting",
                            "start": {"dateTime": "2024-03-08T10:00:00Z"},
                            "end": {"dateTime": "2024-03-08T11:00:00Z"}
                        }
                    },
                    "reset_procedure": [
                        {
                            "clear_calendar": {}
                        }
                    ]
                }
            ]
        },
        {
            "task_id": 5,
            "intent_template": "After the '{event_name}' meeting on {date1}, create a file named '{file}' in the '{folder}' folder. The file should contain '{content}'. Also, schedule a reminder in Google Calendar for {date2} at {time}, with a summary '{summary}', and description '{description}'.",
            "instantiation_dict": {
                "event_name": "Budget Review",
                "date1": "2024-04-15",
                "folder": "tmp/Meeting_Summaries",
                "file": "Budget_Review_Summary_20240415.txt",
                "content": "Summary of Budget Review meeting on 2024-04-15",
                "date2": "2024-04-22",
                "time": "9:00 AM",
                "summary": "Budget Review Follow-up",
                "description": "Reminder to prepare for the follow-up meeting"
            },
            "evals": [
                {
                    "eval_type": "filesystem",
                    "eval_procedure": {
                        "exists": {
                            "tmp/Meeting_Summaries/Budget_Review_Summary_20240415.txt": true
                        },
                        "content_check": {
                            "tmp/Meeting_Summaries/Budget_Review_Summary_20240415.txt": "Summary of Budget Review meeting on 2024-04-15"
                        }
                    },
                    "reset_procedure": [
                        {
                            "rmdir": {
                                "path": "tmp"
                            }
                        },
                        {
                            "mkdir": {
                                "path": "tmp/Meeting_Summaries"
                            }
                        }
                    ]
                },
                {
                    "eval_type": "google_calendar",
                    "eval_procedure": {
                        "event_match": {
                            "summary": "Budget Review Follow-up",
                            "description": "Reminder to prepare for the follow-up meeting",
                            "start": {"dateTime": "2024-04-22T09:00:00Z"},
                            "end": {"dateTime": "2024-04-22T09:30:00Z"}
                        }
                    },
                    "reset_procedure": [
                        {
                            "clear_calendar": {}
                        }
                    ]
                }
            ]
        },
        {
            "task_id": 6,
            "intent_template": "Reschedule the '{event_name}' event originally on '{old_date}' to '{new_date}' at '{time}'.",
            "instantiation_dict": {
                "event_name": "Team Sync",
                "old_date": "2024-05-10",
                "new_date": "2024-05-12",
                "time": "2:00 PM"
            },
            "evals": [
                {
                    "eval_type": "google_calendar",
                    "eval_procedure": {
                        "event_match": {
                            "summary": "Team Sync",
                            "start": {"dateTime": "2024-05-12T14:00:00Z"},
                            "end": {"dateTime": "2024-05-12T15:00:00Z"}
                        },
                        "check_event_exists": {
                            "event": {
                                "summary": "Team Sync",
                                "start": {"dateTime": "2024-05-10T00:00:00Z"}
                            },
                            "exists": false
                        }
                    },
                    "reset_procedure": [
                        {
                            "clear_calendar": {}
                        },
                        {
                            "create_event": {
                                "summary": "Team Sync",
                                "start": {"dateTime": "2024-05-10T00:00:00Z"},
                                "end": {"dateTime": "2024-05-10T01:00:00Z"}
                            }
                        }
                    ]
                }
            ]
        },
        {
            "task_id": 7,
            "intent_template": "Reschedule the '{event_name}' event originally on '{old_date}' to '{new_date}' at '{time}'. Then create a file named '{file}' in the '{folder}' folder, containing '{content}'.",
            "instantiation_dict": {
                "event_name": "Team Sync",
                "old_date": "2024-05-10",
                "new_date": "2024-05-12",
                "time": "2:00 PM",
                "folder": "tmp/Notifications",
                "file": "Team_Sync_Rescheduled_20240512.txt",
                "content": "Team Sync meeting has been rescheduled to 2024-05-12 at 2:00 PM"
            },
            "evals": [
                {
                    "eval_type": "filesystem",
                    "eval_procedure": {
                        "exists": {
                            "tmp/Notifications/Team_Sync_Rescheduled_20240512.txt": true
                        },
                        "type_check": {
                            "tmp/Notifications/Team_Sync_Rescheduled_20240512.txt": "file"
                        },
                        "content_check": {
                            "tmp/Notifications/Team_Sync_Rescheduled_20240512.txt": "Team Sync meeting has been rescheduled to 2024-05-12 at 2:00 PM"
                        }
                    },
                    "reset_procedure": [
                        {
                            "rmdir": {
                                "path": "tmp"
                            }
                        },
                        {
                            "mkdir": {
                                "path": "tmp/Notifications"
                            }
                        }
                    ]
                },
                {
                    "eval_type": "google_calendar",
                    "eval_procedure": {
                        "event_match": {
                            "summary": "Team Sync",
                            "start": {"dateTime": "2024-05-12T14:00:00Z"},
                            "end": {"dateTime": "2024-05-12T15:00:00Z"}
                        },
                        "check_event_exists": {
                            "event": {
                                "summary": "Team Sync",
                                "start": {"dateTime": "2024-05-10T00:00:00Z"}
                            },
                            "exists": false
                        }
                    },
                    "reset_procedure": [
                        {
                            "clear_calendar": {}
                        },
                        {
                            "create_event": {
                                "summary": "Team Sync",
                                "start": {"dateTime": "2024-05-10T00:00:00Z"},
                                "end": {"dateTime": "2024-05-10T01:00:00Z"}
                            }
                        }
                    ]
                }
            ]
        },
        {
            "task_id": 8,
            "intent_template": "Check if there is a '{event_name}' scheduled on '{date}' in Google Calendar. If it exists, create a folder named '{folder}' in '{location}' and inside it, create two files: '{file1}' with content '{content1}' and '{file2}' with content '{content2}'.",
            "instantiation_dict": {
                "event_name": "Project Planning Meeting",
                "date": "2024-08-10",
                "folder": "Project_Planning_20240810",
                "location": "tmp/Meeting_Documents",
                "file1": "Attendees.txt",
                "content1": "List of Attendees",
                "file2": "Agenda.txt",
                "content2": "Meeting Agenda"
            },
            "evals": [
                {
                    "eval_type": "filesystem",
                    "eval_procedure": {
                        "exists": {
                            "tmp/Meeting_Documents/Project_Planning_20240810": true,
                            "tmp/Meeting_Documents/Project_Planning_20240810/Attendees.txt": true,
                            "tmp/Meeting_Documents/Project_Planning_20240810/Agenda.txt": true
                        },
                        "content_check": {
                            "tmp/Meeting_Documents/Project_Planning_20240810/Attendees.txt": "List of Attendees",
                            "tmp/Meeting_Documents/Project_Planning_20240810/Agenda.txt": "Meeting Agenda"
                        }
                    },
                    "reset_procedure": [
                        {
                            "rmdir": {
                                "path": "tmp"
                            }
                        },
                        {
                            "mkdir": {
                                "path": "tmp/Meeting_Documents"
                            }
                        }
                    ]
                },
                {
                    "eval_type": "google_calendar",
                    "reset_procedure": [
                        {
                            "clear_calendar": {}
                        },
                        {
                            "create_event": {
                                "summary": "Project Planning Meeting",
                                "start": {"dateTime": "2024-08-10T09:00:00Z"},
                                "end": {"dateTime": "2024-08-10T11:00:00Z"}
                            }
                        }
                    ]
                }
            ]
        }
    ]
}
