{
    "applications": ["google_calendar", "filesystem"],
    "applications_settings": {
        "google_calendar": {
            "token_path": "token.json"
        },
        "filesystem": {
        }
    },
    "intent": "Add some introductions to this environment",
    "environments": {
        "google_calendar": {
            "enter": [
                {
                    "cd_calendar": {
                        "id": "primary"
                    }
                }
            ]
        },
        "filesystem": {
            "enter": [
                {
                    "mkdir": {
                        "path": "tmp"
                    }
                },
                {
                    "create_file": {
                        "path": "tmp/test.txt",
                        "content": "Hello World!"
                    }
                },
                {
                    "chmod": {
                        "path": "tmp/test.txt",
                        "mode": "0o644"
                    }
                },
                {
                    "chmod": {
                        "path": "tmp",
                        "mode": "0o775"
                    }
                }
            ]
        }
    }
}
