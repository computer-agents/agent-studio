{
  "task_id": "3c8f201a-009d-4bbe-8b65-a6f8b35bb57f",
  "instruction": "Download the image from \"https://drive.google.com/uc?export=download&id=1VIwIhLpkRr72DTuJINsZQ-DDwHAtfddq\", and then use GIMP to compress it to under 600KB as \"compressed.jpeg\" to \"~\". Resize if needed.",
  "visual": true,
  "max_steps": 30,
  "eval_procedure": [
    {
      "evaluator": "gimp",
      "function": "check_image_file_size",
      "params": {
        "src_path": "${AS_ROOT}/compressed.jpeg",
        "rules": {
          "max_size": 600000
        }
      }
    }
  ],
  "reset_procedure": [
    {
      "evaluator": "filesystem",
      "function": "rm",
      "params": {
        "path": "${AS_ROOT}/compressed.jpeg"
      }
    }
  ]
}
